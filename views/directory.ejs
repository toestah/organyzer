<head>
    <%- include('bootstrap.ejs') %>
</head>

<body>
    <%- include('navbar.ejs') %>
    <div class="container">
        <div class="row">
            <table id="myTable" class="display" style="width:100%">
                <thead>
                    <tr>
                        <% for (key of Object.keys(records[0]._fields[0].properties)){ %>
                            <% if (key != "Model"){ %>
                            <th>
                                <%=key%>
                            </th>
                            <% } %>
                        <% } %>
                    </tr>
                </thead>
                <tbody>
                    <% records.forEach((record)=> { %>
                        <tr>
                        <% for ([key,val] of Object.entries(record._fields[0].properties)){ %>
                            <% if (key != "Model"){ %>
                                <td>
                                    <%=val%>
                                </td>
                            <% } %>
                        <% } %>
                        </tr>
                    <% }) %>
                </tbody>
            </table>
        </div>
    </div>
    <%- include('scripts.ejs') %>
    <script>
        $(document).ready(function () {
            $('#myTable').DataTable();
        });

    </script>
</body>